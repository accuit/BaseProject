  <log4net debug="true" info="true" error="true" warn="true">
    <root>
      <level value="ALL" />
    </root>
    <logger name="EShot">
      <level value="DEBUG" />
      <appender-ref ref="EshotProcessorAppender" />
    </logger>
    <appender name="EshotProcessorAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="Logs\\DreamWedds.log" />
      <param name="appendToFile" value="true" />
      <param name="rollingStyle" value="Size" />
      <param name="maxSizeRollBackups" value="100" />
      <param name="maximumFileSize" value="10MB" />
      <param name="staticLogFileName" value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n" />
      </layout>
    </appender>
  </log4net>
  <connectionStrings configSource="Config\Connection.config" />
  <loggingConfiguration configSource="Config\Logging.config" />
  <exceptionHandling configSource="Config\ExceptionPolicies.config" />
  <dataConfiguration defaultDatabase="AccuIT_DB_Connection" />
